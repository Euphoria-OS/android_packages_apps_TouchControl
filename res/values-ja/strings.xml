    <string name="sweep2sleep_title">スワイプでスリープ</string>
    <string name="sweep2sleep_off">無効</string>
    <string name="sweep2sleep_right">右</string>
    <string name="sweep2sleep_left">左</string>
    <string name="sweep2sleep_right_left">右と左</string>

    <!-- Gestures -->
    <string name="gestures_title">ウェイクジェスチャー</string>
    <string name="gestures_summary">このオプションはウェイクジェスチャー機能を有効にするだけであり、ウェイクジェスチャーXposedモジュールをインストールしてジェスチャーアクションを追加する必要があります。</string>

    <!-- Powerkey suspend -->
    <string name="powerkeysuspend_title">電源キーでサスペンド</string>
    <string name="powerkeysuspend_summary">電源キーを使用して画面をオフにすると一時的に全てのオプションを無効にする</string>

    <!-- Wake timeout -->
    <string name="wake_timeout_title">ウェイクタイムアウト</string>
    <string name="wake_timeout_summary">このタイムアウトの後にオプションを一時的に無効にする</string>

    <!-- Vibration strength -->
    <string name="vibration_strength_title">バイブの強さ</string>
    <string name="vibration_strength_summary">振動フィードバックの強さを調整する</string>

    <!-- Touch wake -->
    <string name="touchwake_title">タッチウェイク</string>
    <string name="touchwake_summary">タイムアウト後に画面をタッチするだけでデバイスをウェイクアップさせる</string>

    <!-- Touch wake timeout -->
    <string name="touchwake_timeout_title">タッチウェイクタイムアウト</string>
    <string name="touchwake_timeout_summary">このタイムアウトの後にオプションを一時的に無効にする</string>

