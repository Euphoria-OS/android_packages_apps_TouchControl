    <!-- Set on boot -->
    <string name="set_on_boot_title">起動時に適用</string>
    <string name="set_on_boot_summary">起動時に設定を復元する</string>

    <!-- Double tap to wake -->
    <string name="doubletap2wake_title">ダブルタップでウェイク</string>
    <string name="doubletap2wake_summary">画面の任意の場所をダブルタップすることでデバイスをウェイクアップさせる</string>
    <string name="doubletap2wake_off">無効</string>
    <string name="doubletap2wake_bottom">画面の下半分</string>
    <string name="doubletap2wake_full">画面全体</string>

    <!-- Sweep to wake -->
    <string name="sweep2wake_title">スワイプでウェイク</string>
    <string name="sweep2wake_off">無効</string>
    <string name="sweep2wake_right">右</string>
    <string name="sweep2wake_left">左</string>
    <string name="sweep2wake_up">上</string>
    <string name="sweep2wake_down">下</string>
    <string name="sweep2wake_right_left">右と左</string>
    <string name="sweep2wake_right_up">右と上</string>
    <string name="sweep2wake_right_down">右と下</string>
    <string name="sweep2wake_left_up">左と上</string>
    <string name="sweep2wake_left_down">左と下</string>
    <string name="sweep2wake_up_down">上と下</string>
    <string name="sweep2wake_all">全ての方向</string>

